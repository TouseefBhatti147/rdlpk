pipeline{
   agent any
   stages{
      stage('login server'){
         steps{
            sshagent(credentials:['WebApplication_Dev_Key']){
               sh 'ssh  -o StrictHostKeyChecking=no  ubuntu@3.1.41.8 /home/jenkins/build_rdlpk.sh'
          }
        echo "success Build"
         }
       }
   }
}